{% extends 'base.html.twig' %}

{% block body %}
    <h1>Crear Laboratorio</h1>

    {{ form_start(form) }}
        <div class="form-group col-md-12">
            {{ form_label(form.nombre) }}
            {{ form_widget(form.nombre) }}
            {{ form_errors(form.nombre) }}
        </div>
        <div class="form-group col-md-12">
            {{ form_label(form.contacto) }}
            {{ form_widget(form.contacto) }}
            {{ form_errors(form.contacto) }}
        </div>
        <div class="form-group col-md-12">
            {{ form_label(form.telcontacto) }}
            {{ form_widget(form.telcontacto) }}
            {{ form_errors(form.telcontacto) }}
        </div>
        <div class="form-group col-md-12">
                {{ form_label(form.save) }}
        {{ form_end(form) }}
                <a href="{{ path('laboratorio_index') }}" class="btn btn-danger col-md-4 pull-right">Cancelar</a>
        </div>    
{% endblock %}
