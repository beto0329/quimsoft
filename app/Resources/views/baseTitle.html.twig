<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}QuimSoft{% endblock %}</title>
        {% block stylesheets %}
            {% stylesheets 'bundles/app/css/*' filter='cssrewrite' %}
                <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <style>
            .fixed-bottom {
                position: fixed;
                left: 0;
                bottom: 0;
                width: 100%;
                text-align: center;
                z-index: 999999;
            }
            
        </style>
    </head>
    <body>
            <nav class="navbar navbar-default navbar-fixed-top "> 
                <div class="container-fluid"> 
                    <div class="navbar-header">
                        <a class="navbar-brand" href="#">
                             {% image '@AppBundle/Resources/public/css/images/logo.svg' %}
                                <img src="{{ asset_url }}" alt="logo" width="30" />
                            {% endimage %} 
                        </a>
                    </div>
                    <a href="#" class="pull-left navbar-brand" >QUIMSOFT</a>
                </div>
            </nav> 
            <div class="container-fluid">

                    {% block body %}{% endblock %}        

            </div>
        <div class="fixed-bottom">
            <a href="#" class="pull-left" >
                 {% image '@AppBundle/Resources/public/css/images/logo.svg' %}
                    <img src="{{ asset_url }}" alt="logo" width="50" />
                {% endimage %} <span style="color: #fff;">Powered By QuimSoft</span>
            </a>
            
        </div>
        {% block javascripts %}
            {% javascripts 
                '@AppBundle/Resources/public/js/jquery-2.1.1.min.js'
                '@AppBundle/Resources/public/js/jquery.dataTables.min.js'
                '@AppBundle/Resources/public/js/dataTables.buttons.min.js'
                '@AppBundle/Resources/public/js/bootstrap.min.js'
                '@AppBundle/Resources/public/js/bootstrap-select.js'
                '@AppBundle/Resources/public/js/moment.js'
                '@AppBundle/Resources/public/js/bootstrap-datetimepicker.js'%}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
</html>
